# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Authentication
VITE_VIP_CODE=your_secure_vip_code

# AI Services - Hybrid AI Engine (Client-side accessible)
VITE_OPENAI_API_KEY=your_openai_api_key
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key

# Development Settings
VITE_BYPASS_AUTH=false
NODE_ENV=development

# Stripe (if using payments)
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_SECRET_KEY=your_stripe_secret_key

# Email Service
RESEND_API_KEY=your_resend_api_key

# Analytics
VITE_GA_TRACKING_ID=your_google_analytics_id

# Security
VITE_CORS_ORIGIN=http://localhost:5173

# Server Configuration
PORT=3001
LOG_LEVEL=info

# Database
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# AI Services (Server-side only - NEVER expose with VITE_ prefix)
ANTHROPIC_API_KEY=sk-ant-api03-your_key_here
OPENAI_API_KEY=sk-your_openai_key_here

# Media Storage (S3-compatible)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
S3_BUCKET=fansworld-media
S3_ENDPOINT=https://your-s3-endpoint.com
CDN_BASE_URL=https://cdn.fansworld.com

# Media Configuration
MAX_FILE_SIZE=104857600
MAX_IMAGE_SIZE=10485760
MAX_VIDEO_SIZE=104857600
MAX_AUDIO_SIZE=20971520
IMAGE_QUALITY=85
THUMBNAIL_SIZE=400
WATERMARK_ENABLED=true
VIRUS_SCAN_ENABLED=false

# Frontend URLs
VITE_SITE_URL=http://localhost:3000
FRONTEND_URL=http://localhost:3000

# Real-time Messaging
REALTIME_PROVIDER=socket.io
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password
PUSHER_APP_ID=your_pusher_app_id
PUSHER_KEY=your_pusher_key
PUSHER_SECRET=your_pusher_secret
PUSHER_CLUSTER=us2

# Message Configuration
MAX_MESSAGE_LENGTH=2000
MAX_ATTACHMENTS=5
MESSAGE_ENCRYPTION_ENABLED=true
MESSAGE_RETENTION_DAYS=365

# Payment Integration (Stripe)
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret