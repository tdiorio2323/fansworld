# This file serves as an example for the required environment variables.
# Copy this file to .env and fill in your actual credentials.
# DO NOT commit the .env file to version control.

# ----------------------------------------------------------------
# Supabase Credentials (as per project documentation)
# ----------------------------------------------------------------
# Find these in your Supabase project dashboard under Project Settings > API
NEXT_PUBLIC_SUPABASE_URL="https://your-project-ref.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-supabase-anon-key"

# ----------------------------------------------------------------
# Neon Database Credentials (from your input)
# ----------------------------------------------------------------
# If you are using Neon instead of or in addition to Supabase DB

# Recommended for most uses
DATABASE_URL="postgres://<user>:<password>@<host-pooler>/<database>?sslmode=require"

# For uses requiring a connection without pgbouncer
DATABASE_URL_UNPOOLED="postgresql://<user>:<password>@<host>/<database>?sslmode=require"

# Parameters for Vercel Postgres Templates
POSTGRES_URL=""
POSTGRES_URL_NON_POOLING=""
POSTGRES_USER=""
POSTGRES_HOST=""
POSTGRES_PASSWORD=""
POSTGRES_DATABASE=""
POSTGRES_PRISMA_URL=""

# ----------------------------------------------------------------
# Neon Auth / "Stack" Credentials (from your input)
# ----------------------------------------------------------------
NEXT_PUBLIC_STACK_PROJECT_ID=""
NEXT_PUBLIC_STACK_PUBLISHABLE_CLIENT_KEY=""
STACK_SECRET_SERVER_KEY=""