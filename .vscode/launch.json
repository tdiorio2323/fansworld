{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run esbuild",
            "runtimeExecutable": "esbuild",
            "cwd": "${workspaceFolder}/node_modules/vite",
            "args": []
        },
        {
            "name": "Launch and Debug Standalone Blazor WebAssembly App",
            "type": "blazorwasm",
            "request": "launch",
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Run tailwindcss",
            "runtimeExecutable": "tailwindcss",
            "cwd": "${workspaceFolder}",
            "args": []
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Run esbuild",
            "runtimeExecutable": "esbuild",
            "cwd": "${workspaceFolder}/node_modules/lovable-tagger",
            "args": []
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Run esbuild",
            "runtimeExecutable": "esbuild",
            "cwd": "${workspaceFolder}/node_modules/lovable-tagger",
            "args": []
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Run eslint",
            "runtimeExecutable": "eslint",
            "cwd": "${workspaceFolder}",
            "args": []
        },
        {
            "name": "Launch index.html",
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "file": "${workspaceFolder}/index.html"
        },
        {
            "name": "Launch localhost",
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "url": "http://localhost/index.html",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Attach",
            "type": "firefox",
            "request": "attach"
        },
        {
            "name": "Launch WebExtension",
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "addonPath": "${workspaceFolder}"
        }
    ]
}