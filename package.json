{
  "name": "fansworld-fresh-new",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"vite\"",
    "dev:client": "vite",
    "server:dev": "tsx watch api/server.ts",
    "server:build": "tsc --project tsconfig.server.json",
    "build": "vite build",
    "lint": "eslint . && tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest run --exclude='**/tests/**' --exclude='**/*.spec.ts'",
    "test:unit": "vitest run src/",
    "test:watch": "vitest --exclude='**/tests/**' --exclude='**/*.spec.ts'",
    "test:coverage": "vitest run --coverage --exclude='**/tests/**' --exclude='**/*.spec.ts'",
    "test:e2e": "playwright test tests/",
    "smoke": "playwright test tests/smoke.spec.ts",
    "generate:vip-codes": "node scripts/generate-vip-codes.js",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel"
  },
