{
  "name": "creator-automation-scripts",
  "version": "1.0.0",
  "description": "Creator Management Platform Automation Scripts",
  "type": "module",
  "main": "creator-automation.js",
  "bin": {
    "creator-automation": "./creator-automation.js"
  },
  "scripts": {
    "start": "node creator-automation.js schedule",
    "sync": "node creator-automation.js sync",
    "sync:force": "node creator-automation.js sync --force",
    "report:monthly": "node creator-automation.js report --type monthly",
    "report:weekly": "node creator-automation.js report --type weekly",
    "status": "node creator-automation.js status",
    "install:global": "npm install -g .",
    "setup": "chmod +x creator-automation.js && npm install"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.6.2",
    "commander": "^11.1.0",
    "node-cron": "^3.0.3",
    "stripe": "^14.9.0",
    "puppeteer": "^21.6.1",
    "jspdf": "^2.5.1",
    "nodemailer": "^6.9.7",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "creator",
    "automation",
    "analytics",
    "instagram",
    "tiktok",
    "youtube",
    "metrics",
    "reporting"
  ],
  "author": "TD Studios",
  "license": "MIT"
}
